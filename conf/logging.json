{
	"version": 1,
	"disable_existing_loggers": false,
	"formatters": {
		"default": {
			"format": "'[{asctime}] [{levelname:<8}] {name}: {message}",
			"datefmt": "%Y-%m-%d %H:%M:%S",
			"style": "{"
		}
	},
	"handlers": {
		"file": {
			"class": "logging.handlers.RotatingFileHandler",
			"level": "DEBUG",
			"formatter": "default",
			"filename": "logs/discord.log",
			"maxBytes": 33554432,
			"backupCount": 128,
			"encoding": "utf-8"
		}
	},
	"loggers": {
		"discord.http": {
			"level": "INFO"
		}
	}
}